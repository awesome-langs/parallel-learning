#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/elm-posix/bin/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/elm-posix/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/elm-posix/bin/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/elm-posix/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules/@albertdahlin/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/@albertdahlin+elm-posix@1.1.0/node_modules:/home/nixos/parallel-learning/Elm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@albertdahlin/elm-posix/bin/elm-cli" "$@"
else
  exec node  "$basedir/../@albertdahlin/elm-posix/bin/elm-cli" "$@"
fi
